#!/usr/bin/env bash

# shellcheck source=SCRIPTDIR/../../../../../../../../jehon/usr/bin/jh-lib
. jh-lib

case "$1" in
"home")
    header_begin "Starting transmission-daemon"
    systemctl start transmission-daemon
    header_end
    ;;

*)
    header_begin "Stopping transmission-daemon"
    systemctl stop transmission-daemon
    header_end
    ;;
esac
